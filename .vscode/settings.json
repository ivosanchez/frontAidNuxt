{
  "files.exclude": {
    ".nuxt": true,
    ".vscode": true,
    "README.md": true,
    "yarn.lock": true,
    "jsconfig.json": true,
    "node_modules": true,
    "LICENSE": true,
    ".gitattributes": true,
    ".gitignore": true
  },
  "workspace.isHidden": false,
  "workbench.colorCustomizations": {
    "[DW-Theme]": {
      "editorGutter.commentRangeForeground": "#ff0000",
      "activityBar.activeBorder": "#ff0000",
      "badge.background": "#ff0000",
      "breadcrumb.background": "#000",
      "breadcrumb.focusForeground": "#fff",
      "editorGroupHeader.tabsBorder": "#9ed191",
      "editor.foreground": "#000",
      "icon.foreground": "#fff",
      "terminal.ansiGreen": "#ff0000",
      "window.activeBorder": "#ff0000",
      "editorInfo.foreground": "#fff",
      "scrollbar.shadow": "#000",
      "todomd.commentForeground": "#ff0000",
      "editor.selectionBackground": "#ff0000",
      "editor.selectionForeground": "#fff",
      "breadcrumb.foreground": "#fff"
    },
    "tab.activeBorder": "#afefe1",
    "titleBar.activeBackground": "#afefe1",
    "titleBar.activeForeground": "#000000",
    "activityBar.background": "#afefe1",
    "activityBar.foreground": "#000000",
    "statusBar.background": "#afefe1",
    "statusBar.foreground": "#000000"
  },
  "window.title": "~[vue]~${separator}${separator}${activeEditorShort}${separator}${rootName}"
}